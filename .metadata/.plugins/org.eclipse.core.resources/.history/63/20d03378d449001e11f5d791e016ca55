
public class Main {

	public static void main(String[] args) {
		int a = 2;
		int b = 6;
		int c = 4;
		int delta = Math.pow(b, 2.0) - 4 * a * c;
		int x1 = - b + Math.sqrt(delta)/2* a;
		int x2 = - b - Math.sqrt(delta) / 2 * a;
		System.out.printf("x1 = %s" + x1);
		System.out.printf("x2 = %s" + x2);
		
		

	}

}
