
public class Main {

	public static void main(String[] args) {
		double a = 2;
		double b = 6;
		double c = 4;
		double delta = Math.pow(b, 2.0) - 4 * a * c;
		double x1 = (-(b) + (Math.sqrt(delta))) / (2 * a);
		double x2 = (-(b) - (Math.sqrt(delta))) / (2 * a);
		System.out.println(x1);
		System.out.println(x2);
		
		

	}

}
